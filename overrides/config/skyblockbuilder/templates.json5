{
  // The template which will be used for spawn only
  // Example: 
  // {
  //     "name": "default",
  //     "desc": "",
  //     "file": "default.nbt",
  //     "spawns": "default",
  //     "direction": "south"
  // }
  "spawn": null,
  
  "spawns": {
    "default": [
      [ 6, 3, 5 ]
    ],
    "gog": [
      [ 3, 6, 2 ]
    ]
  },
  
  // The list of templates being available. The first entry is the default template.
  "templates": [
    {
      "name": "Classic",
      "desc": "Classic Skyblock",
      "file": "default.nbt",
      "spawns": "default",
      "direction": "south"
    },
    {
      "name": "Garden of Glass",
      "desc": "Garden of Glass from Botania",
      "file": "gog.nbt",
      "spawns": "gog",
      "direction": "south"
    }
  ]
}
